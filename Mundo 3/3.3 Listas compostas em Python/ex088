from random import randint
from time import sleep
l = []
s = []
q = int(input('Quantos jogos vocÃª quer gerar? '))
t = 1
while t <= q:
    c = 0
    while True:
        n = randint(1,60)
        if n not in l:
            l.append(n)
            c += 1
        if c == 6:
            break
    l.sort()
    s.append(l[:])
    l.clear()
    t += 1
for i,v in enumerate(s):
    print (f'Jogo {i+1}: {v}')
    sleep(1)
