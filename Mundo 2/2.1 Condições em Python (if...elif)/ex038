n1 = int(input('Digite o primeiro valor: '))
n2 = int(input('Digite o segundo valor: '))
if n1 > n2:
    print('-=' * 10)
    print (f'{n1} é maior que {n2}.')
    print('-=' * 10)
elif n2 > n1:
    print('-=' * 10)
    print (f'{n2} é maior que {n1}.')
    print('-=' * 10)
elif n1 == n2:
    print('-=' * 10)
    print (f'{n1} é igual a {n2}.')
    print('-=' * 10)
