t = int(input('Digite o primeiro termo: '))
r = int(input('Digite a razão: '))
c = s = 0
a = 10
while a != 0:
    s = s + a
    while c < s:
        print(t, end=' -> ')
        t += r
        c += 1
    print ('FIM.')
    a = int(input('Quantos termos você quer ver a mais?'))
print ('Fim do programa.')
